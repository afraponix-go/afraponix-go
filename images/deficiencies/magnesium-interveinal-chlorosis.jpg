<svg width="300" height="200" xmlns="http://www.w3.org/2000/svg">
  <!-- Leaf with magnesium deficiency - interveinal chlorosis -->
  <ellipse cx="150" cy="100" rx="120" ry="60" fill="#e6d836" stroke="#2d3a1f" stroke-width="2"/>
  
  <!-- Green veins (characteristic of magnesium deficiency) -->
  <path d="M 50 100 Q 150 80 250 100" stroke="#2d5a1f" stroke-width="4" fill="none"/>
  <path d="M 80 120 Q 150 100 220 120" stroke="#2d5a1f" stroke-width="3" fill="none"/>
  <path d="M 80 80 Q 150 100 220 80" stroke="#2d5a1f" stroke-width="3" fill="none"/>
  <path d="M 100 110 Q 150 100 200 110" stroke="#2d5a1f" stroke-width="2" fill="none"/>
  <path d="M 100 90 Q 150 100 200 90" stroke="#2d5a1f" stroke-width="2" fill="none"/>
  
  <!-- Secondary green veins -->
  <path d="M 120 85 L 135 100 L 120 115" stroke="#2d5a1f" stroke-width="2" fill="none"/>
  <path d="M 180 85 L 165 100 L 180 115" stroke="#2d5a1f" stroke-width="2" fill="none"/>
  <path d="M 110 100 L 130 85" stroke="#2d5a1f" stroke-width="1" fill="none"/>
  <path d="M 110 100 L 130 115" stroke="#2d5a1f" stroke-width="1" fill="none"/>
  <path d="M 190 100 L 170 85" stroke="#2d5a1f" stroke-width="1" fill="none"/>
  <path d="M 190 100 L 170 115" stroke="#2d5a1f" stroke-width="1" fill="none"/>
  
  <!-- Yellowish interveinal areas -->
  <polygon points="90,85 120,85 110,100 90,100" fill="#f4e836" fill-opacity="0.8"/>
  <polygon points="180,85 210,85 200,100 180,100" fill="#f4e836" fill-opacity="0.8"/>
  <polygon points="90,115 120,115 110,100 90,100" fill="#f4e836" fill-opacity="0.8"/>
  <polygon points="180,115 210,115 200,100 180,100" fill="#f4e836" fill-opacity="0.8"/>
  
  <!-- Text label -->
  <text x="150" y="180" text-anchor="middle" font-family="Arial" font-size="14" fill="#333">
    Magnesium Deficiency - Interveinal Chlorosis
  </text>
</svg>